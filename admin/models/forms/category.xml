<?xml version="1.0" encoding="utf-8"?><form><!--Project : SCET for Joomla Filename: /admin/models/forms/category.xmlVersion : 2.5.19--><field  name = "id"            type = "hidden"/><!-- Felder: name, ordering, published --><field    name        = "ordering"     type        = "text"    label       = "COM_SCET_ORDERING"     description = "COM_SCET_ORDERING_DESC"      default     = "0"     class       = "inputbox"></field><field    name        = "published"     type        = "radio"    label       = "COM_SCET_PUBLISHED"     description = "COM_SCET_PUBLISHED_DESC"      default     = "1"     class       = "btn-group">    <option value="1">JYES</option>    <option value="0">JNO</option></field><field    name        = "publiccategory"     type        = "radio"    label       = "COM_SCET_PUBLIC"     description = "COM_SCET_PUBLIC_DESC"      default     = "1"     class       = "btn-group">    <option value="1">JYES</option>    <option value="0">JNO</option></field><field  name        = "category"          type        = "text"          label       = "COM_SCET_CATEGORY_NAME"         description = "COM_SCET_CATEGORY_NAME_DESC"          class       = "inputbox"          size        = "128"           required    = "true" />&lt; br /&gt;<field  name        = "preamble"          type        = "editor"          label       = "COM_SCET_PREAMBLE"         description = "COM_SCET_PREAMBLE_DESC"          class       = "inputbox"          size        = "128"   /></form>