<?xml version="1.0" encoding="utf-8"?><form><!--Project : SCET for Joomla Filename: /admin/models/forms/event.xmlVersion : 3.0.0--><field  name = "id"            type = "hidden"/><!-- Felder: Event, Ort, Kategorie, Ã¶ffentlich, freigegeben, mandatory, datum, uhrzeit, updated, inserted --><field  name        = "event"          type        = "text"          label       = "COM_SCET_EVENT"         description = "COM_SCET_EVENT_DESC"          class       = "inputbox"          size        = "128"           required    = "true" /><field  name        = "rule"          type        = "text"          label       = "COM_SCET_RULE"         description = "COM_SCET_RULE_DESC"          class       = "inputbox"          size        = "128"   /><field  name        = "location"          type        = "text"          label       = "COM_SCET_LOCATION"         description = "COM_SCET_LOCATION_DESC"          class       = "inputbox"          size        = "128"   /><field  name    = "fk_category"         type    = "categories"        label   = "COM_SCET_CATEGORY"         default = "0"         class   = "inputbox"        size    = "1"></field><field  name        = "published"         type        = "radio"        label       = "COM_SCET_PUBLISHED"        description = "COM_SCET_PUBLISHED_DESC"          default     = "0"         class       = "btn-group">    <option value="1">JYES</option>    <option value="0">JNO</option></field><field  name        = "mandatory"         type        = "radio"        label       = "COM_SCET_MANDATORY"         description = "COM_SCET_MANDATORY_DESC"          default     = "0"         class       = "btn-group">    <option value="1">JYES</option>    <option value="0">JNO</option></field><field  name        = "publicevent"         type        = "radio"        label       = "COM_SCET_PUBLIC"         description = "COM_SCET_PUBLIC_DESC"          default     = "0"         class       = "btn-group">    <option value="1">JYES</option>    <option value="0">JNO</option></field><field  name        = "anniversary"         type        = "radio"        label       = "COM_SCET_ANNIVERSARY"         description = "COM_SCET_ANNIVERSARY_DESC"          default     = "0"         class       = "btn-group">    <option value="1">JYES</option>    <option value="0">JNO</option></field><field  name        = "datum"        type        = "calendar"        label       = "COM_SCET_DATE"        description = "COM_SCET_DATE_DESC"          class       = "inputbox"          size        = "40"        format      = "%d.%m.%Y"/><field  name        = "uhrzeit"          type        = "text"          label       = "COM_SCET_TIME"         description = "COM_SCET_TIME_DESC"          class       = "inputbox"          size        = "40"   /><field  name        = "endezeit"          type        = "text"          label       = "COM_SCET_ENDTIME"         description = "COM_SCET_ENDTIME_DESC"          class       = "inputbox"          size        = "40"   /><field  name        = "inserted"        type        = "calendar"        label       = "COM_SCET_INSERTED"        description = "COM_SCET_INSERTED_DESC"          size        = "40"        class       = "inputbox"        readonly    = "true"        format      = "%d.%m.%Y %H:%M:%S"/><field  name        = "updated"        type        = "calendar"        label       = "COM_SCET_UPDATED"        description = "COM_SCET_UPDATED_DESC"          size        = "40"        class       = "inputbox"        readonly    = "true"        format      = "%d.%m.%Y %H:%M:%S"/></form>